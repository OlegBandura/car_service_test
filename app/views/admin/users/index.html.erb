<table class="user_table">
  <tr>
    <th onclick="sort_car_table(0)">Username</th>
    <th onclick="sort_car_table(1)">Type user</th>
    <th onclick="sort_car_table(2)">Email</th>
    <th onclick="sort_car_table(3)">Sign in count</th>
    <th onclick="sort_car_table(5)">Last sign in</th>
    <th onclick="sort_car_table(4)">Created account</th>
    <th>Options</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.username%></td>
    <td><% if user.admin == true %>
      <p>administrator</p>
      <% else %>
      <p></p>
      <% end %>
    </td>
    <td><%= user.email%></td>
    <td><%= user.sign_in_count%></td>
    <td><%= user.last_sign_in_at%></td>
    <td><%= user.created_at%></td>

    <th><%= link_to "Edit" %> | <%= link_to "Delete", method: :delete, data: {confirm: 'Are you sure?' } %>

  </tr>
  <% end %>
</table>
